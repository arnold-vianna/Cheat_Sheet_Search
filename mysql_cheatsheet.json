{
    "SHOW DATABASES;": "List all databases",
    "USE database_name;": "Select a database to use",
    "SHOW TABLES;": "List all tables in the current database",
    "DESCRIBE table_name;": "Show the structure of a table",
    "SELECT column_name FROM table_name;": "Select data from a table",
    "SELECT * FROM table_name;": "Select all columns from a table",
    "INSERT INTO table_name (column1, column2, ...) VALUES (value1, value2, ...);": "Insert data into a table",
    "UPDATE table_name SET column1 = value1, column2 = value2 WHERE condition;": "Update data in a table",
    "DELETE FROM table_name WHERE condition;": "Delete data from a table",
    "CREATE DATABASE database_name;": "Create a new database",
    "DROP DATABASE database_name;": "Delete a database",
    "CREATE TABLE table_name (column1 datatype, column2 datatype, ...);": "Create a new table",
    "DROP TABLE table_name;": "Delete a table",
    "ALTER TABLE table_name ADD column_name datatype;": "Add a column to a table",
    "ALTER TABLE table_name DROP COLUMN column_name;": "Delete a column from a table",
    "ALTER TABLE table_name MODIFY COLUMN column_name datatype;": "Modify the data type of a column",
    "CREATE INDEX index_name ON table_name (column_name);": "Create an index on a table",
    "DROP INDEX index_name ON table_name;": "Delete an index from a table",
    "CREATE USER 'username'@'host' IDENTIFIED BY 'password';": "Create a new user",
    "GRANT permission ON database_name.table_name TO 'username'@'host';": "Grant permissions to a user",
    "REVOKE permission ON database_name.table_name FROM 'username'@'host';": "Revoke permissions from a user",
    "SHOW GRANTS FOR 'username'@'host';": "Show the permissions granted to a user",
    "RENAME TABLE old_table_name TO new_table_name;": "Rename a table",
    "TRUNCATE TABLE table_name;": "Delete all data from a table",
    "BACKUP DATABASE database_name TO DISK = 'filepath';": "Backup a database",
    "RESTORE DATABASE database_name FROM DISK = 'filepath';": "Restore a database from a backup",
    "CREATE VIEW view_name AS SELECT column1, column2 FROM table_name WHERE condition;": "Create a new view",
    "DROP VIEW view_name;": "Delete a view",
    "CREATE PROCEDURE procedure_name() BEGIN ... END;": "Create a stored procedure",
    "DROP PROCEDURE procedure_name;": "Delete a stored procedure",
    "CREATE FUNCTION function_name(param1 datatype, param2 datatype) RETURNS datatype BEGIN ... END;": "Create a stored function",
    "DROP FUNCTION function_name;": "Delete a stored function",
    "CREATE TRIGGER trigger_name BEFORE INSERT ON table_name FOR EACH ROW BEGIN ... END;": "Create a trigger",
    "DROP TRIGGER trigger_name;": "Delete a trigger",
    "START TRANSACTION;": "Start a transaction",
    "COMMIT;": "Commit a transaction",
    "ROLLBACK;": "Rollback a transaction",
    "SET autocommit = 0;": "Disable autocommit",
    "SET autocommit = 1;": "Enable autocommit",
    "LOCK TABLES table_name READ;": "Lock a table for reading",
    "LOCK TABLES table_name WRITE;": "Lock a table for writing",
    "UNLOCK TABLES;": "Unlock all tables",
    "CHECK TABLE table_name;": "Check a table for errors",
    "REPAIR TABLE table_name;": "Repair a table",
    "OPTIMIZE TABLE table_name;": "Optimize a table",
    "ANALYZE TABLE table_name;": "Analyze a table",
    "CHECKSUM TABLE table_name;": "Compute a checksum for a table",
    "FLUSH TABLES;": "Close all tables",
    "FLUSH TABLES table_name;": "Close a table",
    "FLUSH TABLES WITH READ LOCK;": "Lock all tables for reading",
    "FLUSH PRIVILEGES;": "Reload the privileges from the grant tables",
    "CHANGE MASTER TO master_option;": "Change the master server parameters",
    "STOP SLAVE;": "Stop the slave thread",
    "START SLAVE;": "Start the slave thread",
    "RESET SLAVE;": "Reset the slave connection info",
    "RESET MASTER;": "Reset the master server info",
    "PURGE BINARY LOGS;": "Purge binary logs",
    "PURGE MASTER LOGS;": "Purge master logs",
    "SET GLOBAL general_log = ON;": "Enable the general query log",
    "SET GLOBAL general_log = OFF;": "Disable the general query log",
    "SET GLOBAL slow_query_log = ON;": "Enable the slow query log",
    "SET GLOBAL slow_query_log = OFF;": "Disable the slow query log",
    "SET GLOBAL long_query_time = time;": "Set the long query time",
    "SHOW GLOBAL VARIABLES;": "Show global variables",
    "SHOW GLOBAL STATUS;": "Show global status",
    "SHOW ENGINE INNODB STATUS;": "Show InnoDB status",
    "SHOW PROCESSLIST;": "Show the process list",
    "KILL processlist_id;": "Kill a process",
    "SHOW OPEN TABLES;": "Show open tables",
    "SHOW PLUGINS;": "Show plugins",
    "INSTALL PLUGIN plugin_name SONAME 'plugin_file';": "Install a plugin",
    "UNINSTALL PLUGIN plugin_name;": "Uninstall a plugin",
    "SHOW CHARACTER SET;": "Show character sets",
    "SHOW COLLATION;": "Show collations",
    "SHOW CREATE DATABASE database_name;": "Show the CREATE DATABASE statement",
    "SHOW CREATE TABLE table_name;": "Show the CREATE TABLE statement",
    "SHOW CREATE VIEW view_name;": "Show the CREATE VIEW statement",
    "SHOW CREATE PROCEDURE procedure_name;": "Show the CREATE PROCEDURE statement",
    "SHOW CREATE FUNCTION function_name;": "Show the CREATE FUNCTION statement",
    "SHOW CREATE TRIGGER trigger_name;": "Show the CREATE TRIGGER statement",
    "SHOW CREATE EVENT event_name;": "Show the CREATE EVENT statement",
    "SHOW CREATE USER 'username'@'host';": "Show the CREATE USER statement",
    "SHOW INDEX FROM table_name;": "Show indexes from a table",
    "SHOW TABLE STATUS;": "Show table status",
    "SHOW FULL PROCESSLIST;": "Show the full process list",
    "SHOW PROFILES;": "Show profiles",
    "SHOW PROFILE;": "Show profile",
    "SHOW PROFILE FOR QUERY query_id;": "Show profile for a query",
    "SET PROFILING = ON;": "Enable profiling",
    "SET PROFILING = OFF;": "Disable profiling",
    "EXPLAIN SELECT column_name FROM table_name;": "Explain a SELECT statement"
